{
  "name": "famulous-app",
  "version": "v1.0.0",
  "license": "MIT",
  "main": "dist/app/index.js",
  "engineStrict": true,
  "engines": {
    "node": "=12.16.3"
  },
  "scripts": {
    "install:all": "yarn && yarn --cwd server && yarn --cwd client",
    "start:dev": "yarn install:all && pm2 start pm2.config.js",
    "stop:dev": "pm2 delete pm2.config.js",
    "monit:dev": "pm2 monit",
    "start": "yarn --cwd server start",
    "clean": "./scripts/clean.sh",
    "test": "./scripts/test.sh",
    "build": "yarn clean && ./scripts/build.sh"
  },
  "dependencies": {
    "pm2": "^4.4.0"
  }
}
